{
	"name": "npmtrends-presets",
	"version": "1.0.0",
	"description": "NPM packages trend comparison presets sharing service",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "npm run build:frontend && npm run build:worker",
		"build:frontend": "vite build",
		"build:worker": "tsc --project tsconfig.worker.json --noEmit && echo 'Worker TypeScript check complete'",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"dev:full": "concurrently \"npm run wrangler:dev\" \"npm run dev\"",
		"wrangler:dev": "npm run build:frontend && wrangler dev",
		"db:create": "wrangler d1 create trends_list",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:migrate:local": "wrangler d1 execute trends_list --file=./schema.sql --local",
		"db:studio": "drizzle-kit studio",
		"deploy": "npm run build && node scripts/deploy.js"
	},
	"dependencies": {
		"@clerk/clerk-react": "^5.40.0",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@hono/clerk-auth": "^3.0.3",
		"@hono/node-server": "^1.18.1",
		"@mui/icons-material": "^7.3.1",
		"@mui/material": "^7.3.1",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.31.4",
		"drizzle-orm": "^0.44.4",
		"hono": "^4.9.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.22.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250810.0",
		"@types/react": "^18.2.56",
		"@types/react-dom": "^18.2.19",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"@vitejs/plugin-react": "^4.2.1",
		"concurrently": "^9.2.0",
		"cors": "^2.8.5",
		"eslint": "^8.56.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"express": "^5.1.0",
		"typescript": "^5.2.2",
		"vite": "^7.1.1",
		"wrangler": "^4.28.1"
	},
	"keywords": [
		"npm",
		"trends",
		"packages",
		"comparison",
		"cloudflare",
		"react"
	],
	"author": "Your Name",
	"license": "MIT"
}
