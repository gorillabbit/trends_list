{
  "name": "npmtrends-presets",
  "version": "1.0.0",
  "description": "NPM packages trend comparison presets sharing service",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:frontend && npm run build:worker",
    "build:frontend": "vite build",
    "build:worker": "tsc --project tsconfig.worker.json --noEmit && echo 'Worker TypeScript check complete'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "mock-api": "node mock-server.js",
    "dev:full": "concurrently \"npm run wrangler:dev\" \"npm run dev\"",
    "wrangler:dev": "npm run build:frontend && wrangler dev",
    "wrangler:deploy": "npm run build && wrangler deploy",
    "db:create": "wrangler d1 create trends_list",
    "db:migrate": "wrangler d1 execute trends_list --file=./schema.sql --local",
    "setup:dev": "./setup-dev-env.sh",
    "setup:all": "npm run setup:dev && echo '\\n次に本番用の.env.prodファイルを作成し、npm run deploy:prod でCloudflareにデプロイしてください'",
    "sync:secrets": "node scripts/sync-secrets.js",
    "sync:secrets:prod": "node scripts/sync-secrets.js prod",
    "deploy": "node scripts/deploy.js",
    "deploy:prod": "node scripts/deploy.js prod"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.1",
    "hono": "^4.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250810.0",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^5.1.0",
    "typescript": "^5.2.2",
    "vite": "^7.1.1",
    "wrangler": "^4.28.1"
  },
  "keywords": [
    "npm",
    "trends",
    "packages",
    "comparison",
    "cloudflare",
    "react"
  ],
  "author": "Your Name",
  "license": "MIT"
}
