{
  "name": "npmtrends-presets",
  "version": "1.0.0",
  "description": "NPM packages trend comparison presets sharing service",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "mock-api": "node mock-server.js",
    "dev:full": "concurrently \"npm run mock-api\" \"npm run dev\"",
    "wrangler:dev": "wrangler pages dev dist --compatibility-date=2025-08-01",
    "wrangler:deploy": "wrangler pages deploy dist",
    "db:create": "wrangler d1 create npmtrends_presets",
    "db:migrate": "wrangler d1 execute npmtrends_presets --file=./schema.sql"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^5.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "wrangler": "^3.48.0"
  },
  "keywords": [
    "npm",
    "trends",
    "packages",
    "comparison",
    "cloudflare",
    "react"
  ],
  "author": "Your Name",
  "license": "MIT"
}
